{
  "id": null,
  "title": "Intelligent Support System - Business KPIs",
    "tags": ["support-system", "business", "kpi"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Customer Satisfaction Score",
        "type": "stat",
        "targets": [
          {
            "expr": "customer_satisfaction_score",
            "legendFormat": "CSAT Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 3
                },
                {
                  "color": "green",
                  "value": 4
                }
              ]
            },
            "unit": "short",
            "min": 1,
            "max": 5
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Ticket Volume by Priority",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (priority) (business_kpi_tickets_by_priority)",
            "legendFormat": "{{priority}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "High Value Customer Tickets",
        "type": "stat",
        "targets": [
          {
            "expr": "business_kpi_high_value_customers",
            "legendFormat": "High Value Tickets"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Ticket Resolution Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(business_kpi_resolution_time_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(business_kpi_resolution_time_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Product Issue Distribution",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum by (product) (business_kpi_tickets_by_product)",
            "legendFormat": "{{product}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "System Performance Impact",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(business_kpi_system_performance[5m])",
            "legendFormat": "Performance Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "30s"
}