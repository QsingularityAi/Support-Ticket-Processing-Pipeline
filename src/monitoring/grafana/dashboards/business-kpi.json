{
  "dashboard": {
    "id": null,
    "title": "Business KPI Dashboard",
    "tags": ["business", "kpi", "support-system"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total Tickets Processed",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "ticket_volume_total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Ticket Volume Over Time",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 18,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(ticket_volume_total[5m])",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Tickets per second"
          }
        ]
      },
      {
        "id": 3,
        "type": "piechart",
        "title": "Tickets by Category",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "ticket_categories_total",
            "legendFormat": "{{category}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "type": "piechart",
        "title": "Tickets by Priority",
        "gridPos": {
          "x": 8,
          "y": 4,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "ticket_priorities_total",
            "legendFormat": "{{priority}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "type": "piechart",
        "title": "Tickets by Severity",
        "gridPos": {
          "x": 16,
          "y": 4,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "ticket_severities_total",
            "legendFormat": "{{severity}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Customer Satisfaction Score",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 4
        },
        "targets": [
          {
            "expr": "customer_satisfaction_score",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Ticket Resolution Time Distribution",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(ticket_resolution_time_hours_bucket[5m]))",
            "legendFormat": "Median",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ticket_resolution_time_hours_bucket[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "B"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "h",
            "label": "Resolution Time (hours)"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  },
  "overwrite": true
}
