{
  "dashboard": {
    "id": null,
    "title": "System Components Monitoring",
    "tags": ["system", "monitoring", "support-system"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "HTTP Requests Over Time",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status_code}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Requests per second"
          }
        ]
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Current HTTP Requests In Progress",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "http_requests_in_progress",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 3,
        "type": "graph",
        "title": "HTTP Request Duration",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          }
        ]
      },
      {
        "id": 4,
        "type": "piechart",
        "title": "HTTP Requests by Status Code",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "http_requests_total",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "type": "graph",
        "title": "API Health Status",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 24,
          "h": 4
        },
        "targets": [
          {
            "expr": "up{job='support-system-api'}",
            "legendFormat": "API Health",
            "refId": "A"
          },
          {
            "expr": "up{job='mlflow'}",
            "legendFormat": "MLflow Health",
            "refId": "B"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Up (1) / Down (0)"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  },
  "overwrite": true
}
