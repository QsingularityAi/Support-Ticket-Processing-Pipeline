{
  "dashboard": {
    "id": null,
    "title": "Model Performance Dashboard",
    "tags": ["ml", "performance", "support-system"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Model Prediction Requests Over Time",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(model_predictions_total[5m])",
            "legendFormat": "{{target}} - {{model_type}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Requests per second"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Model Prediction Duration",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(model_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "{{target}} - {{model_type}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          }
        ]
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Model Predictions",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(model_predictions_total)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 4,
        "type": "piechart",
        "title": "Predictions by Target",
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "model_predictions_total",
            "legendFormat": "{{target}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "type": "graph",
        "title": "Solution Retrieval Requests",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(solution_retrievals_total[5m])",
            "legendFormat": "{{query_type}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Requests per second"
          }
        ]
      },
      {
        "id": 6,
        "type": "graph",
        "title": "Anomaly Detection Over Time",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(anomalies_detected_total[5m])",
            "legendFormat": "{{anomaly_type}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Anomalies per second"
          }
        ]
      },
      {
        "id": 7,
        "type": "stat",
        "title": "Total Anomalies Detected",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(anomalies_detected_total)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 8,
        "type": "piechart",
        "title": "Anomalies by Type",
        "gridPos": {
          "x": 18,
          "y": 12,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "anomalies_detected_total",
            "legendFormat": "{{anomaly_type}}",
            "refId": "A"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  },
  "overwrite": true
}
